[
  {
    "name"        : "butterfree",
    "script"      : "./bin/www",
    "watch"       : false,
    "instances"   : "1",
    "exec_mode"   : "cluster",
    "env": {
      "NODE_ENV": "production",
      "PORT": "3102"
    },
    "node_args": ["--inspect=0.0.0.0:9002"],
    "out_file": "./logs/app.log",
    "error_file": "./logs/app.log",
    "merge_logs": true,
    "log_date_format": "YYYY-MM-DD HH:mm Z"
  },
  {
    "name"        : "butterfree-test",
    "script"      : "./bin/www",
    "watch"       : false,
    "instances"   : "1",
    "exec_mode"   : "cluster",
    "env": {
      "NODE_ENV": "development",
      "PORT": "3103"
    },
    "node_args": ["--inspect=0.0.0.0:9003"],
    "out_file": "./logs/app.log",
    "error_file": "./logs/app.log",
    "merge_logs": true,
    "log_date_format": "YYYY-MM-DD HH:mm Z"
  }
]
