<script src="https://cdn.bootcss.com/datatables/1.10.16/js/jquery.dataTables.min.js" charset="utf-8"></script>
<script type="text/javascript">
$.fn.serializeObject = function() {
 var o = {};
 var a = this.serializeArray();
 $.each(a, function() {
   if (o[this.name]) {
     if (!o[this.name].push) {
       o[this.name] = [o[this.name]];
     }
     o[this.name].push(this.value || '');
   } else {
     o[this.name] = this.value || '';
   }
 });
 return o;
};
</script>
