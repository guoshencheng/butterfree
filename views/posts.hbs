<style media="screen">
  a {
    color: #333333;
  }
  .fa {
    margin: 3px;
  }
</style>
<div class="box">
  <div class="box-header">
    <div class="box-title">{{title}}</div>
  </div>
  <div class="box-header">
    <button type="button" class="btn btn-primary create-post">创建</button>
  </div>
  <div class="box-body">
    <table class="table table-bordered" id="posts-table" >
      <thead>
        <tr>
          <th>编号</th>
          <th>标题</th>
          <th>分类名</th>
          <th>内容片段</th>
          <th>状态</th>
          <th>操作</th>
        </tr>
      </thead>
    </table>
  </div>
</div>
<script src="/js/posts.js?t={{timestamp}}" charset="utf-8"></script>
